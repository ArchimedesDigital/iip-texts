                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="6">
                           <p>The inscription was erased over its entire height and 0.50 of its
                              width. About sixteen to twenty letters in each line are obliterated
                              and two to four characters survive at the beginning and at the end of
                              most lines</p>
                        </layout>
