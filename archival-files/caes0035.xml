                  <textLang mainLang="la"/>
                  <msItem ana="#other_religion" class="text_other">
                     <p>Caesarea, Second or third century CE. Slab. Other text (Senatorial
                        cursus).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#slab">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>13</height>
                              <width>18</width>
                              <depth atLeast="1.8" atMost="2.0">1.8-2</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Broken on all sides. The back has traces of plaster, perhaps from
                              secondary use. </p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="2">
                           <p>Small wedges are used as word dividers, and traces of red paint remain
                              in the letters</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
