               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="la"/>
                  <msItem ana="#other_religion" class="#dedicatory">
                     <p>Caesarea, Second or third century CE. Column. Dedicatory.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#column">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>41</height>
                              <width>53</width>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Fragment of the top of a column, including a necking ring 6-7 cm wide.
                              Two dowel holes 5 cm square survive at the top. The surface of the stone is dressed with a claw chisel</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p>The text starts 15 cm below the top. Dots divide words, all of which
                              are abbreviated.</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="7.6" atMost="8"/>
                        <p>Letters are large and well cut, with serifs. The alphabet is of the second or third century</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab/>
                        <locus/>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njtk9m" notBefore="0100" notAfter="0300">Second or third century CE</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p> Found along the shore south of exit of the large vaulted sewer located
                        south of the point where the Late Antique wall reaches the sea. In the 1980
                        it was in a resident's garden south of Sdot</p>
                  </origin>
                  <provenance>
                     <placeName> Unknown</placeName>
                     <date/>
                  </provenance>
               </history>
            </msDesc>
