               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="la"/>
                  <msItem class="#dedicatory" ana="#other_religion">
                     <p>Caesarea, 117 CE - 138 CE (probably ca. 130 CE). Block. Dedicatory.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#block">
                     <supportDesc ana="#limestone">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>91</height>
                              <width>59</width>
                           </dimensions>
                           <p>The block is built into the softer kurkar masonry of the high level
                              acqueduct</p>
                        </support>
                        <condition ana="#complete">
                           <p>Very poorly preserved</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="4">
                           <p>The inscription was poorly cut, partly because the surface is small
                              and convex</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="3.2" atMost="5.5"/>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab> Eagle within aedicula, head missing. </ab>
                        <locus>Above the medallion</locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab> Winged Victory standing on globe, missing head.  </ab>
                        <locus> Below the medallion. </locus>
                     </decoNote>
                     <decoNote>
                        <ab>Wreathed convex medallion</ab>
                        <locus>Around the inscription</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njbxb9" notBefore="0117" notAfter="0138">117 CE - 138 CE (probably ca. 130
                        CE)</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>Bet Hanania south of Tel Mevorakh and about 50 m east of the beginning of
                        the diversion, on the northern face of the high level aqueduct (Channel B).
                        Caes0052 is adjacent on right.</p>
                  </origin>
                  <provenance>
                     <placeName/>
                     <date/>
                  </provenance>
               </history>
            </msDesc>
