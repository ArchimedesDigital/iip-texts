         <div type="translation" ana="#b1" xml:id="caes0132.translation" corresp="#caes0132.diplomatic">
            <p>Lord <rs type="divine">Jesus Christ </rs> help the <rs type="admTitle">deacon </rs>
               <name>
                  <persName xml:lang="grc " ref="persons.xml#caes0132p01">Chrysos </persName>
