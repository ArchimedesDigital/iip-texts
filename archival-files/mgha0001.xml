                    <physDesc>
                        <objectDesc ana="#block">
                            <supportDesc ana="#stone.other">
                                <support>
                                    <p>Basalt</p>
                                    <dimensions type="surface" unit="cm">
                                        <height>35</height>
                                        <width>190</width>
                                        <depth>17</depth>
                                    </dimensions>
                                </support>
                                <condition ana="#fragments.contig">
