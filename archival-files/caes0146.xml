               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#protome">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>98</height>
                              <width>60</width>
                              <depth>34</depth>
                           </dimensions>
                           <p>Dimensions refer to the whole object. The inscription is on a panel
                              0.54 m wide and 0.45 m high below the torso of the figure.</p>
                        </support>
                        <condition ana="#complete.broken">
                           <p>The head is broken off and the edges are battered</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="6">
                           <p/>
                        </layout>
                     </layoutDesc>
