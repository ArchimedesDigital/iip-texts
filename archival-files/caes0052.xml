               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="la"/>
                  <msItem class="#dedicatory" ana="other_religion">
                     <p>Caesarea, 117 CE - 138 CE (probably ca. 130 CE). Block. Dedicatory. </p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#block">
                     <supportDesc ana="limestone">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>64</height>
                              <width>102</width>
                           </dimensions>
                        </support>
                        <condition ana="complete">
                           <p>The inscription was poorly cut and is badly preserved</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="6">
                           <p>The inscribed area is 46 cm wide and 48 cm high, bordered by a shallow
                              molding</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="4.2" atMost="7.5"/>
                        <p>The letters, with serifs, belong to the monumental alphabet of the first or second centuries</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Tabula ansata</ab>
                        <locus>Around the inscription</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njbxb9" notBefore="0117" notAfter="0138">117 CE - 138 CE (probably ca. 130
                        CE)</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>Found in 1973 in Bet Hanania, south of Tel Mevorakh and about 50 m east of the
                        beginning of the diversion, on the northern face of the high level aqueduct
                        (Channel B). Caes0053 is adjacent on left.</p>
                  </origin>
                  <provenance>
                     <placeName> same </placeName>
                     <date/>
                  </provenance>
               </history>
            </msDesc>
         </sourceDesc>
