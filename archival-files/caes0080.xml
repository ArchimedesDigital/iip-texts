                  </supplied>
               </expan>
               <supplied reason="lost">
                  <gap reason="illegible" quantity="1" unit="character"/>πρ</supplied>οσ<lb break="no"/>
		  φορὰ<lb/>
               <expan>
                  <abbr>το</abbr>
                  <ex>ῦ</ex>
               </expan> λα<lb break="no"/>
		  οῦ ἐπὴ Μ<lb break="no"/>
