                           <dimensions type="surface" unit="cm">
                              <height>24</height>
                              <width>20</width>
                              <depth atLeast="3.0" atMost="4.0">3-4</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>The top and left sides are broken away, taking about six letters from
