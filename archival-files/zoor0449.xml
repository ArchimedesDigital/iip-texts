                    <!--  *************************************  <physDesc>  ********************************* -->
                    <physDesc>
                        <objectDesc ana="#tombstone">
                            <supportDesc ana="#stone.other">
                                <support>
                                    <p>Yellowish sandstone, almost rectangular in shape, slightly
                                        chipped all around</p>
                                    <dimensions type="surface" unit="cm">
                                        <height>38</height>
                                        <width>22</width>
                                        <depth>9</depth>
                                    </dimensions>
                                </support>
                                <condition ana="#complete.broken">
                                    <p>The front surface is weathered with significant loss of text
                                        especially in the middle of the inscription</p>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="1" writtenLines="8">
                                    <p/>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <!--  *************************************  <handDesc>  *********************************  -->
                        <handDesc>
                            <handNote ana="#impressed.inscribed">
                                <p>Traces of red paint are preserved. The letters are small and
                                    neatly executed, in lines improperly spaced and aligned</p>
                            </handNote>
                        </handDesc>
                        <!--  *************************************  <decoDesc>  *********************************  -->
                        <decoDesc>
                            <decoNote>
                                <ab>Faint traces of an engraved frame</ab>
                                <locus>Surrounding text and symbols, it can be barely discernible on
                                    left side.</locus>
                            </decoNote>
                            <decoNote>
                                <ab>Seven-branched candelabrum</ab>
                                <locus>Below the inscription</locus>
                            </decoNote>
                            <decoNote>
                                <ab>Palm branches</ab>
                                <locus>Below the inscription, to either side of the
                                    candelabrum</locus>
                            </decoNote>
                        </decoDesc>
