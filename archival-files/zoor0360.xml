               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem class="#funerary.epitaph" ana="#christian">
                     <p>Zoora, Late 4th to early 5th century CE. Tombstone. Funerary (Epitaph).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#tombstone">
                     <supportDesc ana="#stone.other">
                        <support>
                           <p>White sandstone, rectangular in shape</p>
                           <dimensions type="surface" unit="cm">
                              <height>37</height>
                              <width>27.5</width>
                              <depth>6</depth>
                           </dimensions>
                        </support>
                        <condition ana="#complete.broken">
                           <p>The inscribed surface is flaked off at the upper right corner and
                              weathered along the right side</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p>Slightly incised guide-lines visible under each line</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#impressed.inscribed">
                        <dimensions type="letter" extent="height" atLeast="1.7" atMost="4.6"/>
                        <p>Square script with big and elongated letters, nicely executed and
                           aligned</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Deeply engraved large linear cross</ab>
                        <locus>Below the epitaph. The upper vertical bar separates the third line of
                           the text in two</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Small linear cross</ab>
                        <locus>Above the inscription</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4" notBefore="0375" notAfter="0425">Late 4th to early 5th century CE</date>
                     <placeName>
                        <region>Negev</region>
                        <settlement ref="http://pleiades.stoa.org/places/697768">Zoora</settlement>
                        <geogName type="site">An Naq Cemetery</geogName>
                        <geogFeat type="locus"/>
                     </placeName>
                     <p>Found by local inhabitants in the northwest corner of the Bronze Age,
                        Byzantine and Islamic cemetery in the An Naq neighborhood south of the Wadi
                        al-Hasa, probably in secondary use in later graves.</p>
                  </origin>
                  <provenance>
                     <placeName>Department of Antiquities of Jordan</placeName>
                     <date/>
                  </provenance>
               </history>
            </msDesc>
