                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="3.8" atMost="5.5"/>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Sagum</ab>
                        <locus>Gathered above the right shoulder</locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>Curved dashes or wedges</ab>
                        <locus>Dividing all words to the middle of line 4, thereafter only a
                           few</locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>7-shaped abbreviation, standing for the word "centurio"</ab>
                        <locus>line 3</locus>
                     </decoNote>
                  </decoDesc>
