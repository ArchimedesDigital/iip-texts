                  <textLang mainLang="la"/>
                  <msItem ana="#other_religion" class="#honorific">
                     <p>Caesarea, Late first to third century CE. Statue base. Honorific.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#statue_base">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>83</height>
                              <width>66</width>
                           </dimensions>
                        </support>
                        <condition ana="complete.broken">
                           <p>The base is damaged on its edges, and the surface is scratched and
                              gouged. The stone was further damaged after it was found and the first
                              four letters of line 1 are now missing. </p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="4">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="5.8" atMost="10.8"/>
                        <p>Dots or short dashes mark most abbreviations. Elongated alphabet of the second and third
                           centuries. Letters have serifs.</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab/>
                        <locus/>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njtk9m http://n2t.net/ark:/99152/p0m63njjcn4" notBefore="0075" notAfter="0300">Late first to third century CE</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>Found at Maioumas (Shuni), about 6 km from Caesarea</p>
                  </origin>
                  <provenance>
                     <placeName>Rockefeller Museum, IAA inv. no. 32-2894</placeName>
                     <date/>
                  </provenance>
               </history>
